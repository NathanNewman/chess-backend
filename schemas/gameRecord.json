{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 4,
      "maxLength": 20
    },
    "result": {
      "type": "string"
    },
    "elo": {
      "type": "string"
    },
    "moves": {
      "type": "array",
      "minLength": 1
    },
    "userColor": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": ["username", "result", "elo", "moves", "userColor"]
}
